page {
  width: 100%;
  height: 100%;
  background: #fff;
}



.car-type-title {
  color: #333;
  font-size: 32rpx;
  margin-bottom: 10rpx;
  padding: 0 28rpx;
}

.car-type {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid rgb(204, 204, 204);
  margin-bottom: 50rpx;
  margin-top: 28rpx;
}

.car-type image {
  width: 40rpx;
  height: 40rpx;
}

.car-left {
}

.carinput {
  float: left;
  color: rgb(102, 102, 102);
  width: 500rpx;
}

.add-car-id-box {
  margin-top: 40rpx;
  padding: 0 28rpx 50rpx;
  
}

.add-car-title {
  font-size: 32rpx;
  color: #333;
  margin-bottom: 20rpx;
}

.add-listitem {
  display: flex;
}

.add-car-normal {
  width: 200rpx;
  height: 60rpx;
  border: 1rpx solid rgb(10, 138, 254);
  color: rgb(10, 138, 254);
  line-height: 60rpx;
  text-align: center;
}

.active {
  background: rgb(10, 138, 254) !important;
  color: #fff !important;
}

.add-car-id {
  display: flex;
  align-items: center;
  margin-top: 40rpx;
}

.id-item {
  display: block;
  width: 60rpx;
  height: 60rpx;
  color: #333;
  font-size: 28rpx;
  font-weight: 300;
  line-height: 60rpx;
  text-align: center;
  border: 1rpx solid rgb(255, 133, 82);
  margin-right: 20rpx;
}

.submit-add {
  height: 80rpx;
  width: 702rpx;
  margin: 80rpx auto 0rpx;
  background: rgb(10, 138, 254);
  color: #fff;
  border-radius: 8rpx;
  text-align: center;
  line-height: 80rpx;
}

.warn {
  font-size: 30rpx;
  color: rgb(128, 128, 128);
  display: inline-block;
  margin-left: 20rpx;
}

.more-btn {
  width: 702rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: rgb(10, 138, 254);
  color: #fff;
  text-align: center;
  margin: 10rpx auto;
  border-radius: 8rpx;
}


