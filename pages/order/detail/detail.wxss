page {
  background: #fff;
}

.detail-box {
}

.detail-top {
  margin: 20rpx 22rpx;
  border-bottom: 1rpx solid rgb(244, 244, 244);
  display: flex;
  flex-direction: column;
  padding-bottom: 30rpx;
}

.detail-item {
  display: flex;
  line-height: 60rpx;
}

.detail-name {
  font-size: 28rpx;
  color: rgb(255, 133, 82);
}

.detail-car-id {
  font-size: 24rpx;
  width: 180rpx;
  height: 50rpx;
  border-radius: 8rpx;
  line-height: 50rpx;
  text-align: center;
  border: 1rpx solid rgb(255, 133, 82);
  margin-left: 25rpx;
}

.detail-worker {
  font-size: 26rpx;
  color: rgb(51, 51, 51);
  line-height: 40rpx;
  margin-top: 10rpx;
}

.detail-time {
  font-size: 24rpx;
  color: rgb(204, 204, 204);
  line-height: 40rpx;
}

.detail-orderid {
  font-size: 24rpx;
  color: rgb(204, 204, 204);
  line-height: 40rpx;
}

.detail-footer {
  margin: 10rpx 22rpx;
  border-bottom: 1rpx solid rgb(244, 244, 244);
  padding-bottom: 30rpx;
}

.footer-title {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 20rpx;
}

.pay-method {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  line-height: 50rpx;
}

.pay-left {
  color: rgb(204, 204, 204);
}

.pay-right {
  color: #333;
}

.detiail-sum {
  float: right;
  display: flex;
  margin-right: 22rpx;
  margin-top: 10rpx;
}

.discount {
  margin-left: 30rpx;
}

.discounted {
  font-size: 26rpx;
  color: rgb(204, 204, 204);
  display: inline-block;
  margin-left: 10rpx;
}

.rmb {
  font-size: 26rpx;
  color: #333;
}

.sum {
  font-size: 36rpx;
  color: #333;
}
.empty{
  font-size: 36rpx;
  color: rgb(102, 102, 102);
  font-weight: 400;
  margin-top: 250rpx;
  text-align: center;
}