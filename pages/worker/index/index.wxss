page{
  background: #fff;
}
.worker-list{
  border-bottom:1rpx solid rgb(204, 204, 204);
  height: 286rpx;
  display: flex;
  width: 100%;
  margin: 0 auto;
  align-items:center;
  padding: 0 50rpx;
   
}
.worker-ranking{
  font-size:100rpx;
  font-weight: 400;
  color: rgb(255, 133, 82);

}
.worker-img{
  margin-left:  60rpx;

}
.worker-img image{
  width: 150rpx;
  height: 150rpx;
  border-radius:50%; 
}
.worker-info{
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
}
.worker-name{
font-size: 28rpx;
color: #333;
margin-bottom: 10rpx;

}
.worker-level{
  font-size: 20rpx;
  color: rgb(170, 170, 170);
  display: inline-block;
  margin-left:12rpx; 
}
.worker-bag{
  font-size: 28rpx;
  color: #333;
  margin-top: 10rpx;
}
.worker-order{
  font-size: 20rpx;
  color: rgb(102, 102, 102);
  display: block;
  margin-top: 20rpx;
}
.i-rate-text{
  color: rgb(255, 133, 82) !important;
}